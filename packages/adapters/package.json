{
  "name": "@bloom/adapters",
  "version": "1.0.0",
  "description": "Framework adapters for Bloom authentication",
  "scripts": {
    "build": "tsc && tsc-alias",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@bloom/core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@vitest/ui": "^3.2.4",
    "express": "^4.18.2",
    "next": "^15.5.4",
    "tsc-alias": "^1.8.16",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "next": "^15.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "exports": {
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  }
}
