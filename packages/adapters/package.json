{
  "name": "@bloom/adapters",
  "version": "1.0.0",
  "description": "Framework adapters for Bloom authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@bloom/core": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "express": "^4.18.2"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "exports": {
    "./express": {
      "types": "./dist/express/index.d.ts",
      "require": "./dist/express/index.js",
      "default": "./dist/express/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "express": ["./dist/express/index.d.ts"]
    }
  }
